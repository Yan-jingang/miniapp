.address{
  background-color: white;
  padding: 20rpx;
  border-bottom: 1rpx solid gainsboro;
}
.shopInfo{
  background-color: white;
}
.info_item{
  padding: 20rpx;
  border-bottom: 1rpx solid gainsboro;
  position: relative;
}
.contant{
  display: flex;
  justify-content: space-between;
}
.feedback button{
  position: absolute;
  bottom: 0;
  left: 0;
  opacity: 0;
  width: 100%;
  height: 100%;
}
.exit{
  position: fixed;
  bottom: 0;
  left: 0;
  height: 90rpx;
  width: 100%;
  background-color: red;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
  border-radius: 45rpx;
}