.label{
  width: 200rpx;
}
.getname{
  display: flex;
  padding: 30rpx 8rpx;
  border-bottom: 1rpx solid gainsboro;
}
.choose{
  font-weight: 800;
  position: absolute;
  right: 20rpx;
}
.choose button{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.submit{
  position: absolute;
  bottom: 0;
  left: 0;
  height: 90rpx;
  width: 100%;
  border-radius: 45rpx;
  background-color: red;
}
.tip{
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
}
.submit button{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}